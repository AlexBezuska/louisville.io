		<script type="text/javascript">
			function clearTimes(){
				var items = document.getElementsByTagName("time");
				for (var i = 0; i < items.length; i++) {
					items[i].removeAttribute("data-localized");
					//console.log("time: ", i);
				}
				LocalTime.run();
			}
			var intervalId = setInterval(clearTimes, 6000);
		</script>
