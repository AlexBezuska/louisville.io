<div class="event">
	<div class="event-title">
		<div class="event-and-group">
			<h2><a href="{{URL}}">{{SUMMARY}}</a></h2>
			<h3><a href="{{groupUrl}}">{{groupName}}</a></h3>
		</div>
		<div class="group-logo">
			<a href="{{groupUrl}}">
				<img src="group-icons/{{group}}.png" alt="{{groupName}}" />
			</a>
		</div>
	</div>
	<p>{{DESCRIPTION}}</p>
	<div class="location">
		<a href="http://maps.google.com/?q={{mapQuery}}">{{LOCATION}}</a>
	</div>
	<div class="start-time">
		<time data-format="%A, %B %e, %Y" data-local="time" datetime="{{jsonDate}}">{{startDate}}</time>
		<br />
		<time data-format="@ %l:%M%P" data-local="time" datetime="{{jsonDate}}">{{startDate}}</time>
	</div>
</div>
