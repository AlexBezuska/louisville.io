{{#each events}}
	<div class="event">
		<div class="group-logo">
			<a href="{{groupUrl}}">
				<img src="group-icons/{{group}}.png" alt="{{groupName}}" />
			</a>
		</div>
		<h2><a href="{{URL}}">{{SUMMARY}}</a></h2>
		<h3><a href="{{groupUrl}}">{{groupName}}</a></h3>
		<p>{{DESCRIPTION}}</p>
    <time data-format="%A, %B %e, %Y @ %l:%M%P"
      data-local="time"
      datetime="{{jsonDate}}">{{startDate}}</time>
	</div>
{{/each}}
